{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": false
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "eqeqeq": [2],
    "indent": [2, 2],
    "max-len": [2, 80, 4],
    "no-console": [0],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-underscore-dangle": [0],
    "camelcase": [2, {"properties": "never"}],

    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ],
  "globals": {

  }
}